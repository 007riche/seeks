<!--
xml schema for info results
-->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="simple_types.xsd"/>

  <xs:element name="options">
    <xs:complexType>
      <xs:sequence>
	<xs:element name="feed"
		    minOccurs="0"
		    maxOccurs="unbounded">
	  <xs:complexType>
	    <xs:sequence>
	      <xs:element name="url"
			  minOccurs="0"
			  maxOccurs="unbounded">
		<xs:complexType>
		  <xs:attribute name="value" type="xs:anyURI"/>
		  <xs:attribute name="default" type="seeks_boolean"/>
		</xs:complexType>
	      </xs:element>
</xs:sequence>
	    <xs:attribute name="name" type="xs:string"/> 
	  </xs:complexType>
	</xs:element>
      </xs:sequence>
      <xs:attribute name="version" type="xs:string"/> 
      <xs:attribute name="code-status" type="seeks_codestatus"/> 
      <xs:attribute name="url-source-code" type="xs:anyURI"/>
      <xs:attribute name="thumbs" type="seeks_boolean"/>
      <xs:attribute name="content-analysis" type="seeks_boolean"/>
      <xs:attribute name="clustering" type="seeks_boolean"/>
    </xs:complexType>
  </xs:element>
</xs:schema>