<!--
xml schema for list of peers
-->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="peers">
    <xs:complexType>
      <xs:sequence>
	<xs:element name="peer"
	    	    minOccurs="0"
		    maxOccurs="unbounded">
	  <xs:complexType>
	    <xs:attribute name="url" type="xs:anyURI"/>
	  </xs:complexType>
	</xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>